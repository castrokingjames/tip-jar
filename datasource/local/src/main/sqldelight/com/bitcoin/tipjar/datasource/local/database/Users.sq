CREATE TABLE Users (
  id INTEGER NOT NULL,
  firstName Text NOT NULL,
  lastName Text NOT NULL,
  alias Text NOT NULL,
  title Text NOT NULL,
  email Text NOT NULL,
  imageUrl Text NOT NULL,
  PRIMARY KEY (id)
);

